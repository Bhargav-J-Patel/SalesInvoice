            function validateKeyPress(e, validSet) {

                var key;
                var keychar;
                var aKeyCode = [9, 13, 37, 38, 39, 40, 96, 97, 98, 99, 100, 101, 102, 103, 104, 105, 110, 112, 113, 114, 115, 116, 117, 118, 119, 120, 121, 122, 123, 190];

                if (window.event || !e.which) // IE 
                    key = e.keyCode; // IE 
                else if (e)
                    key = e.which;   // Netscape 
                else
                    return true;     // no validation

                //alert($.inArray(e.keyCode, aKeyCode));
                if ($.inArray(e.keyCode, aKeyCode) != -1 || e.ctrlKey || e.altKey || e.shiftKey) {
                    return true;
                }

                keychar = String.fromCharCode(key);
                validSet += String.fromCharCode(8);

                if (validSet.indexOf(keychar) < 0)
                    return false;

                return true;
            }

